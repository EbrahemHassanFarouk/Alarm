<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="purple_200">#FFBB86FC</color>
    <color name="purple_500">#FF6200EE</color>
    <color name="purple_700">#FF3700B3</color>
    <color name="teal_200">#FF03DAC5</color>
    <color name="teal_700">#FF018786</color>
    <color name="black">#FF000000</color>
    <color name="white">#FFFFFFFF</color>
    <color name="text_color">#92CFFF</color>
    <color name="transparent">@android:color/transparent</color>


    <style name="NumberPickerStyleInTouch">
        <!--        divider color -->
        <item name="colorControlNormal">@color/transparent</item>
        <item name="android:textSize">@dimen/_30ssp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textColorPrimary">@color/text_color</item>
    </style>

    <style name="NumberPickerStyleReleaseTouch">
        <!--        divider color -->
        <item name="colorControlNormal">@color/transparent</item>
        <item name="android:textSize">@dimen/_30ssp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textColorPrimary">@color/number_color_in_time_picker</item>

    </style>
</resources>